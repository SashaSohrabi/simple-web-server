{
  "name": "wbs-node-ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "imports": {
    "#utils": {
      "development": "./src/utils/index.ts",
      "default": "./dist/utils/index.js"
    },
    "#db": {
      "development": "./src/db/index.ts",
      "default": "./dist/db/index.js"
    },
    "#models": {
      "development": "./src/models/index.ts",
      "default": "./dist/models/index.js"
    },
    "#services": {
      "development": "./src/services/index.ts",
      "default": "./dist/services/index.js"
    }
  },
  "scripts": {
    "dev": "node --env-file=.env.development.local --watch --conditions development src/app.ts",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node --env-file=.env.production.local dist/app.js",
    "prestart:server": "npm run build",
    "start:server": "node --env-file=.env.production.local dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.0.14",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "express": "^5.1.0",
    "mongoose": "^8.19.2"
  }
}
